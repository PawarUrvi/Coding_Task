   "source": [
    "arr=[0,1,2,3,4,5,6,7,9]\n",
    "\n",
    "def missing(arr):\n",
    "    a=len(arr)\n",
    "    \n",
    "    total=a*(a+1)//2\n",
    "    atcual=sum(arr)\n",
    "\n",
    "    missing_num=total-atcual\n",
    "\n",
    "    return missing_num\n",
    "\n",
    "A=missing(arr)\n",
    "print(\"The missing number in the Given Array is:\",A,arr)"
   ]
  }
